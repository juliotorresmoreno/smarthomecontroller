<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<table class="table">
  <thead>
    <tr>
      <th>City</th>
      <th>State</th>
      <th>Name</th>
      <th>State</th>
      <th>Created at</th>
      <th>Update at</th>
      <th>Delete at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.city_id %></td>
        <td><%= city.state %></td>
        <td><%= city.name %></td>
        <td><%= city.state_id %></td>
        <td><%= city.created_at %></td>
        <td><%= city.update_at %></td>
        <td><%= city.delete_at %></td>
        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City', new_city_path %>
