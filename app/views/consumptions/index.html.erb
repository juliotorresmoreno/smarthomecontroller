<p id="notice"><%= notice %></p>

<h1>Consumptions</h1>

<table class="table">
  <thead>
    <tr>
      <th>Consumption</th>
      <th>State</th>
      <th>Device</th>
      <th>Date</th>
      <th>Time</th>
      <th>Consumption</th>
      <th>Created at</th>
      <th>Update at</th>
      <th>Delete at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumptions.each do |consumption| %>
      <tr>
        <td><%= consumption.consumption_id %></td>
        <td><%= consumption.state %></td>
        <td><%= consumption.device_id %></td>
        <td><%= consumption.date %></td>
        <td><%= consumption.time %></td>
        <td><%= consumption.consumption %></td>
        <td><%= consumption.created_at %></td>
        <td><%= consumption.update_at %></td>
        <td><%= consumption.delete_at %></td>
        <td><%= link_to 'Show', consumption %></td>
        <td><%= link_to 'Edit', edit_consumption_path(consumption) %></td>
        <td><%= link_to 'Destroy', consumption, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consumption', new_consumption_path %>
