<p id="notice"><%= notice %></p>

<h1>Users Descs</h1>

<table>
  <thead>
    <tr>
      <th>User desc</th>
      <th>State</th>
      <th>Sex</th>
      <th>City residence</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Cell phone</th>
      <th>Birth date</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_descs.each do |users_desc| %>
      <tr>
        <td><%= users_desc.user_desc_id %></td>
        <td><%= users_desc.state %></td>
        <td><%= users_desc.sex_id %></td>
        <td><%= users_desc.city_residence_id %></td>
        <td><%= users_desc.phone %></td>
        <td><%= users_desc.address %></td>
        <td><%= users_desc.cell_phone %></td>
        <td><%= users_desc.birth_date %></td>
        <td><%= users_desc.user_id %></td>
        <td><%= link_to 'Show', users_desc %></td>
        <td><%= link_to 'Edit', edit_users_desc_path(users_desc) %></td>
        <td><%= link_to 'Destroy', users_desc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users Desc', new_users_desc_path %>
