

<p>
  <strong>ID:</strong>
  <%= @parameter.parameter_id %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @parameter.name %>
</p>

<p>
  <strong>Creado:</strong>
  <%= @parameter.created_at.in_time_zone("Bogota").strftime("%Y-%m-%d %H:%M") %>
