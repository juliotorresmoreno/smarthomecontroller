<p id="notice"><%= notice %></p>

<h1>Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Session</th>
      <th>State</th>
      <th>User</th>
      <th>Token</th>
      <th>Expires</th>
      <th>Created at</th>
      <th>Update at</th>
      <th>Delete at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.session_id %></td>
        <td><%= session.state %></td>
        <td><%= session.user_id %></td>
        <td><%= session.token %></td>
        <td><%= session.expires %></td>
        <td><%= session.created_at %></td>
        <td><%= session.update_at %></td>
        <td><%= session.delete_at %></td>
        <td><%= link_to 'Show', session %></td>
        <td><%= link_to 'Edit', edit_session_path(session) %></td>
        <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session', new_session_path %>
