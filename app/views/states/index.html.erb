<p id="notice"><%= notice %></p>

<h1>States</h1>

<table class="table">
  <thead>
    <tr>
      
      
      <th>Name</th>
      <th>Country</th>
      <th>Creado</th>
      <th>Modificado</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @states.each do |state| %>
      <tr>
        <td><%= state.state_id %></td>
        <td><%= state.state %></td>
        <td><%= state.name %></td>
        <td><%= state.country_id %></td>
        <td><%= state.created_at %></td>
        <td><%= state.update_at %></td>
        
        <td><%= link_to 'Mostrar', state %></td>
        <td><%= link_to 'Editar', edit_state_path(state) %></td>
        <td><%= link_to 'Eliminar', state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State', new_state_path %>
